<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.co.rland.boot3.repository.MemberRepository">

    <select id="findByUsername" resultType="Member">
        select id, username, pwd password, email, reg_date from member where username = #{username}
    </select>

</mapper>